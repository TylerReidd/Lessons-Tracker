  <body>
      <h2 class="table">Set a New Goal!</h2>
      <form class="table" action="/goals" method="POST" > 
    <label> Goal
            <input required name="goal" type="text"><br>
    </label><br>
    <label> Practice Days
        <input required name="daysPracticed" type="text">
    </label><br><br>
    <label>Challenges
        <input required name="challenges" type="text">
        <br><br>
    </label>
    <label> Start Date
        <input required type="date" name="startDate">
    </label><br> <br>
    <label> Due Date
        <input required type="date" name="dueDate">
    </label>
    <br><br>
    <label>Completed?
        <input name="completed" type="checkbox">
    </label>
    <br><br>
    <input type="submit" class="btn btn-info" value="Submit">

    </form>
  </body>

