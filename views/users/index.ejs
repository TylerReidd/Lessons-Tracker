<head>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
  <link rel="stylesheet" href="stylesheets/style.css" />
</head>

<nav>
  <div class="nav-wrapper">
    <a href="/users/goals">My Goals</a>
    <a href="/users/questions" >Ask A Question</a>
    <a href="/">Tuner</a>
    <% if (user) { %>
      <a href="/auth/logout">Log Out</a>
    <% } else { %>
      <a href="/auth/google">Login with Google</a>
    <% } %>
  </div>
</nav>

<script defer src="/javascripts/script.js"></script>
<body>
  <div class="navbar-fixed">
    
  </div>
  <ul>Registered Users:
    <% users.forEach(u => { %>
      <a href="/users<%= u._id %>">
      <%= u.name %></a> - Joined on <% u.createdAt.toLocaleString()
      .split(',')[0] %>
    <% }) %>
  </ul>
<br><br><br><br>

  <h2>Are you in tune? </h2>
  <p>It sounds like you're playing...</p>
  <h1 id="note-name"></h1>
  <p>
      <span>frequency (Hz):</span>
      <span id="frequency"></span>
  </p>
  <hr>
  <button id="play-note">Click for an 'E'</button>
  <hr>
</body>
</html>

