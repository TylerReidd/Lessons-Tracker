<nav>
    <div class="nav-wrapper">
      <a href="/users/goals" class="brand-logo left">My Goals</a>
      <a href="/questions/index" >Ask A Question</a>
      <a href="/">Tuner</a>
    </div>
  </nav>


  <h3>Questions?</h3>
  <form action="/questions" method="POST">
    <div class="form-group">
      <input type="text" class="form-control" aria-describedby="question" name="title" placeholder="Enter message...">
    </div>
    <button type="submit" class="btn btn-success">Post Question</button>
  </form>
  
  <% questions.forEach(question => { %>
    <div class="card">
      <div class="card-header">
        <%= question.askedBy %> >
      </div>
      <div class="card-body">
        <h5 class="card-title"><%= question.title %></h5>
        <a href="/questions/<%= question._id %>" class="btn btn-primary">See Post</a>
      </div>
    </div>
  <% }) %>
  